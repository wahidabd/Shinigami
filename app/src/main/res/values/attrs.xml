<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="HeaderDetail">
        <attr name="iconLeft" format="reference"/>
        <attr name="iconRight1" format="reference"/>
        <attr name="iconRight2" format="reference"/>
        <attr name="iconRight3" format="reference"/>
        <attr name="authorIcon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="MyToolbar">
        <attr name="title" format="string"/>
        <attr name="imgMain" format="reference"/>
        <attr name="imgSecond" format="reference"/>
        <attr name="toolbarType" format="enum">
            <enum name="primary" value="0"/>
            <enum name="transparant" value="1"/>
            <enum name="opacity" value="2"/>
        </attr>
    </declare-styleable>
    
</resources>